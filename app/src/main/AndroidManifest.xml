<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.memsafe.demo">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:extractNativeLibs="true"
        android:theme="@style/Theme.NativeTest"
        >

        <profileable android:shell="true"/>
        <activity
            android:name="com.memsafe.demo.SettingsActivity"
            android:exported="false" />
        <activity
            android:name="com.memsafe.demo.JavaMemLeakActivity"
            android:exported="false" />
        <activity
            android:name="com.memsafe.demo.LeakResourceActivity"
            android:exported="false" />
        <activity
            android:name="com.memsafe.demo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>